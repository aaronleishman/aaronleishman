BoxLayout:
    orientation: 'vertical'
    MDToolbar:
        id:tb
        title: "Aaron's Weather"
        left_action_items:[['icons/icon.png', lambda x: print('icon')]]
        right_action_items: [['magnify', lambda x: app.search_menu.open()]]
    MDBottomNavigation:
        panel_color: [1, 1, 1, 1]
        MDBottomNavigationItem:
            text: "Current"
            id: scn1
            name: "live"
            icon: "death-star"
            ScrollView:
                MDList:
                    TwoLineAvatarListItem:
                        text: app.w.addr
                        secondary_text: 'lat: ' + app.w.lati + ' lon: ' + app.w.long
                        id: l1
                        ImageLeftWidget:
                            source: "icons/map.png"
                    OneLineAvatarListItem:
                        text: app.w.liveDescription.title()
                        id: l2
                        ImageLeftWidget:
                            source: "icons/" + app.w.liveDescriptionIcon +".png"
                    OneLineAvatarListItem:
                        text: app.w.liveTemp + '  |  Feels: ' + app.w.liveFeelsLike
                        id: l3
                        ImageLeftWidget:
                            source: "icons/004-hot.png"
                    OneLineAvatarListItem:
                        text: 'Wind: ' + app.w.liveWindDirection + " " + app.w.liveWindSpeed
                        id: l4
                        ImageLeftWidget:
                            source: "icons/002-wind.png"
                    OneLineAvatarListItem:
                        text: 'Humidity: ' + app.w.liveHumidity
                        id: l5
                        ImageLeftWidget:
                            source: "icons/humidity.png"
                    OneLineAvatarListItem:
                        text: 'Barometer: ' + app.w.livePressure
                        id: l6
                        ImageLeftWidget:
                            source: "icons/001-barometer.png"
                    OneLineAvatarListItem:
                        text: 'Sunrise: ' + app.w.liveSunrise
                        id: l7
                        ImageLeftWidget:
                            source: "icons/003-sunrise.png"
                    OneLineAvatarListItem:
                        text: 'Sunset: ' + app.w.liveSunset
                        id: l8
                        ImageLeftWidget:
                            source: "icons/003-sunrise.png"
                    OneLineAvatarListItem:
                        text: 'As of: ' + app.w.lastRefresh.strftime('%a: %m/%d%l:%M%p')
                        id: l9
                        ImageLeftWidget:
                            source: "icons/refresh.png"
        MDBottomNavigationItem:
            text: "Outlook"
            name: "forecast"
            icon: "space-invaders"
            ScrollView:
                MDList:
                    id: outlook
                    TwoLineAvatarListItem:
                        text: app.w.addr
                        secondary_text: 'lat: ' + app.w.lati + ' lon: ' + app.w.long
                        id: f00
                        ImageLeftWidget:
                            source: "icons/map.png"
                    ThreeLineAvatarListItem:
                        id: f0
                        text: str(app.w.forecastTemp[0]) + 'F' + ' ' +  app.w.forecastDescription[0].title()
                        secondary_text: 'Wd: '  + app.w.windDirection(app.w.forecastWindDeg[0]) + ' ' + str(app.w.forecastWindSpeed[0]) +' MPH' + ' | Hum: ' + str(app.w.forecastHumidity[0]) + "%"
                        tertiary_text: app.w.forecastDateTime[0].strftime('%a: %m/%d/%y %l:%M%p' )
                        ImageLeftWidget:
                            id: f0icon
                            source: 'icons/' + str(app.w.forecastDescIcon[0]) + '.png'
